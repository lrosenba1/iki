
-- This is a fairly long Iki program
-- for the front end tester.  It should
-- pass both syntax and semantics checking.

begin
  var x;
  var y;
  var longName;
  var longNAme;  --case sensitivity test
  write 2, x;    -- multiple write expressions test
  while 4 * x loop
    var x;  -- nested variable declaration okay
    var z;
    while 5 - longName loop
      longNAme = x + z - y;   -- zero declarations test
    end;
    read x, y, z;
  end;
  x = 5 * 4 + 23423 / 3 * 0 + 234 - 333;
end
